      "3000/5000 samples drawn\n",
      "3100/5000 samples drawn\n",
      "3200/5000 samples drawn\n",
      "3300/5000 samples drawn\n",
      "3400/5000 samples drawn\n",
      "3500/5000 samples drawn\n",
      "3600/5000 samples drawn\n",
      "3700/5000 samples drawn\n",
      "3800/5000 samples drawn\n",
      "3900/5000 samples drawn\n",
      "4000/5000 samples drawn\n",
      "4100/5000 samples drawn\n",
      "4200/5000 samples drawn\n",
      "4300/5000 samples drawn\n",
      "4400/5000 samples drawn\n",
      "4500/5000 samples drawn\n",
      "4600/5000 samples drawn\n",
      "4700/5000 samples drawn\n",
      "4800/5000 samples drawn\n",
      "4900/5000 samples drawn\n"
     ]
    }
   ],
   "source": [
    "from copy import deepcopy\n",
    "n_samples = 5000\n",
    "samples = []\n",
    "for i in range(n_samples):\n",
    "    samples.append(deepcopy(sampler.sample()))\n",
    "    if i % 100 == 0:\n",
    "        print '{}/{} samples drawn'.format(i, n_samples)\n",
    "    "
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
