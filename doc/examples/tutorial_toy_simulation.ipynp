    "import matplotlib.pyplot as plt\n",
    "energies = map(lambda x: -posterior.log_prob(**x.variables), samples)\n",
    "plt.plot(energies[100:])\n",
    "plt.xlabel('# of MCMC samples')\n",
    "plt.ylabel('-log(posterior)')\n",
    "plt.show()"
   ]
